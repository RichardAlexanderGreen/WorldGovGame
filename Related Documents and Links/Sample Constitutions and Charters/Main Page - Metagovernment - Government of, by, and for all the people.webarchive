			for ( var k = 0; k < oldClasses.length; k++ ) {
				if ( oldClasses[k] != '' && oldClasses[k] != 'even' && oldClasses[k] != 'odd' ) {
					newClassName += oldClasses[k] + ' ';
				}
			}
			tableRows[j].className = newClassName + ( j % 2 == 0 ? 'even' : 'odd' );
		}
	}
}

