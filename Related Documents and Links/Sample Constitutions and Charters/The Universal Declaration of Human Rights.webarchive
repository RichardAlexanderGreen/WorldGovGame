@media print {
* { background: #fff; color: #000; }
html { font: 100%/1.5 georgia, serif; }

div#content {float:none}

div#udhr {float:none}

div#topbanner {float:none}

div#mainnav {display:none;width:0%; float:none}
.column {float:none}

body {float:none}
}

html {
overflow-y: scroll; /* IE */
min-height: 100%; margin-bottom: 1px;  /* FIREFOX */
}

body {
	font-family: Geneva, Arial, Helvetica, sans-serif;
	margin-top: 0px;
	background-color: #872A09;
	background-repeat: repeat-x;
	background-image: url(images/grad.jpg);
}
	
/* MAIN CONTAINER - HOMEPAGE*/

#main {
	width: 60em;
	margin: 0px auto;
	background-color: #FFFFFF;
	border: 1px solid #300906;
}	

 /* SKIP TO NAVIGATION AND SKIP TO TEXT*/
 
#skiplink {
	padding: 0px;
	margin: 0px;
	clear: both;
	font-size: 10px;
	display: inline;
	color: #C9600D;
}
#skiplink a {
	color:#C9600D;
}
#skiplink a:focus, div#skiplink a:active
{
	color:#F9F1E4;
	background-color: #300906;
}

/* TOP BANNER */

#topbanner {
	clear: none;
	background-image: url(images/grad.jpg);
	background-repeat: repeat-x;
	height:147px;
	
}



#hiddentitle, #hiddentitle span {
	margin: 0px;
	padding: 0px;
	background-image: url(images/banner.jpg);
	background-repeat: no-repeat;
	height: 147px;
}

#hiddentitle span {
	margin-bottom: -75px;
}

h1 {
	font-size: 15px;
	padding:0;
	margin:0;
	text-align: center;
	color: #300906;
}
h1 span {
	display: block;
	position: relative;
	z-index: 1;	
}

 /* LEFT MENU NAVIGATION LINKS - HOMEPAGE*/
#clear {
	width: 176px;
	height: 6px;
	background-color: #E1C893;
	padding-top: 1px;
}

#horiznav  {
	float: left;
	width: 100%;
	background-color: #EE9408;
}

#horiznav ul {	
	margin: 0;
	padding: 0;
}

#horiznav ul li {
list-style-type: none;
float: left;
margin: 0;
}

#horiznav ul li a {
font-weight: bold;
color: #333333;
text-decoration: none;
display: block;
text-align: center;
border-right: 1px solid #e5b64b;
/*padding: 3px 30px; ORIGINAL*/
padding: 3px 47px;
border-bottom: 2px solid #EE9408;
}

#horiznav ul li.first a {
width: 140px;
padding: 3px 0;
}
#horiznav ul li.last a{
width: 138px;
padding: 3px 0;
border: none;
}

#horiznav ul li a:hover {
background-color: #FCE7A4;
text-decoration: none;
border-bottom: 2px solid #660000;
}

#mainnav  {
	float: left;
	width: 18%;
	font-family: Arial, Helvetica, sans-serif;
	background-color: #FBCC60;
	padding-bottom: 20px;
	clear: left;
}

#mainnav ul {
	margin: 0;
	padding: 0;
	text-align: left;
	font-weight: normal;
	list-style-type: none;
}
#mainnav ul li.first {
	margin-top: 6px;
}

#mainnav.column a   {
	margin:0;
	padding: 4px 10px 4px 14px;
	color:#000000;
	text-decoration:none;
	display: block;
	border-bottom-width: 1px;
	border-bottom-style: solid;
	border-bottom-color: #e5b64b;
}

#mainnav.column ul li a:visited{
color:#611f09;
}

#mainnav.column ul li a:hover{
background-color:#FFFFFF;
border-left-color:#ee940a;
border-left-style:solid;
border-left-width:4px;
}


/* Languages */

#languages{
background-color:#df8b07;
}

#main #languages ul li {
color:#FFFFFF;
}
#main #languages ul li a{
color:#7c090c;
}



/* PAGE CONTENT ATTRIBUTES */

#content{
width:80%;
}
#content2{
width: 94%;
margin: 0 auto;
_padding: 0 20px;
}
#content h2, #content2 h2 {
	color: #7F0D0D;	
	font-weight: bold;
	text-transform: uppercase;
	clear: both;
	padding-top: 1em;
}

#content h3, #content2 h3 {
	color: #660000;	
	font-weight: bold;

}
	
#content a:link, #content2 a:link {
	color: #A80E13;
}
#content a:visited, #content2 a:visited {
	color: #513403;
	
}
#content a:hover, #content2 a:hover{ 
	color: #660000;
}
#content a:active,  #content2 a:active {
	color: #513403;
}


#content ul li,  #content2 ul li{
	
	color: #300906;
	list-style-type:disc;
	padding-top: 0;
	padding-right: 0%;
	padding-bottom: 1%;
	padding-left: 1%;
	list-style-position:inside;
}

.photo {
	width: auto;
	float: left;
	margin: 0 0;
	margin-left: none;
	padding-top: 0;
}

.photoright350 {
	width: 350px;
	float: right;
	margin-left: 4em;
	margin-right: 2em;
	margin-bottom: 0;
	padding-top: 0;
}

.photocredit {
	text-align:left;
	padding: 5px 0;
	color: #8B2D0B;
	font-size: 94%;
}


/* BOX QUOTE */
blockquote {
	display: block;
	left: 300px;
	background-color: #FCE7A4;
	padding: 2%;
	margin: 2%;
	border-top-width: 5px;
	border-bottom-width: 1px;
	border-top-style: solid;
	border-bottom-style: solid;
	border-top-color: #EE9408;
	border-bottom-color: #EE9408;
	
}
blockquote p {
	margin: 0px;
	padding: 1% 2% 0 1%;
	line-height: 150%;
	font-size: 0.8em;
}

blockquote .credit {
	text-align: right;
	font-style: oblique;
	display: block;
	padding-right: 2%;
	padding-bottom: 3px;
	font-size: 0.8em;
	margin-top: 12px;
}

.totop p a{
float:right;
width:100%;
padding-right:3px;
clear:both;
}


#footer {
	background-color: #660000;
}

#textbox {
	display: block;
	left: 300px;
	background-color: #fae0b6;
	padding: 2%;
	margin: 0px;
	border-top-width: 5px;
	border-bottom-width: 1px;
	border-top-style: solid;
	border-bottom-style: solid;
	border-top-color: #EE9408;
	border-bottom-color: #EE9408;
}

#textbox h3 {
	color: #660000;
	font-size: 0.75em;
	padding: 0 0 1px;
	font-weight: bold;
	border-bottom-width: 1px;
}

#textbox p {
	margin: 0px;
	padding: 0 1% 0 1%;
	line-height: 150%;
	font-size: 0.8em;
}

#textbox .credit {
	text-align: right;
	font-style: oblique;
	display: block;
	padding-right: 2%;
	padding-bottom: 3px;
	font-size: 0.8em;
	margin-top: 12px;
}

.namebox {
	display: block;
	background-color: #F4D17F;
	padding: 2%;
	margin-left: 10px;
	border-top-width: 5px;
	border-bottom-width: 1px;
	border-top-style: solid;
	border-bottom-style: solid;
	border-top-color: #872A09;
	border-bottom-color: #872A09;
	height: 464px;
	float: left;
	font-family: Georgia, "Times New Roman", Times, serif;
	width: 190px;
